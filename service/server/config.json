{
  "listeners": {
    "*:5100": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": "/generic/*",
        "method": [
          "GET"
        ]
      },
      "action": {
        "pass": "applications/generic_api"
      }
    }
  ],
  "applications": {
    "generic_api": {
      "type": "external",
      "executable": "/opt/app/generic_api",
      "working_directory": "/opt/app",
      "environment": {
        "IS_DEV": "0"
      }
    }
  }
}